<PizzaDough>
    MDBoxLayout:
        orientation: "vertical"
        ScrollView:
            do_scroll_x: False
            MDGridLayout:
                cols: 1
                col_force_default: False
                spacing: dp(6)
                adaptive_height: True
                padding: dp(6)
                MDListItem:
                    MDListItemHeadlineText:
                        text: "Pizza Dough"
                        font_style: "Display"
                        role: "medium"
                MDListItem:
                    MDListItemHeadlineText:
                        text: "Ingredients"
                        font_style: "Headline"
                        role: "medium"
                MDListItem:
                    MDListItemHeadlineText:
                        text: "2-2 1/3 cups Flour"
                    MDListItemSupportingText:
                        text: "all purpose or bread flour"
                    MDListItemTrailingCheckbox:
                MDListItem:
                    MDListItemHeadlineText:
                        text: "2 1/4 tsp Yeast"
                    MDListItemTrailingCheckbox:
                MDListItem:
                    MDListItemHeadlineText:
                        text: "1 1/2 tsp Sugar"
                    MDListItemTrailingCheckbox:
                MDListItem:
                    MDListItemHeadlineText:
                        text: "3/4 tsp Salt"
                    MDListItemTrailingCheckbox:
                MDListItem:
                    MDListItemHeadlineText:
                        text: "2 tbsp Olive oil"
                    MDListItemSupportingText:
                        text: "plus additional"
                    MDListItemTrailingCheckbox:
                MDListItem:
                    MDListItemHeadlineText:
                        text: "3/4 cup of Warm Water"
                    MDListItemTrailingCheckbox:
                MDListItem:
                    MDListItemHeadlineText:
                        text: "Directions"
                        font_style: "Headline"
                        role: "medium"
                MDListItem:
                    MDListItemHeadlineText:
                        text: "Combine 1 cup flour, instant"
                    MDListItemSupportingText:
                        text: "yeast sugar and salt in a large bowl"
                    MDListItemTrailingCheckbox:
                MDListItem:
                    MDListItemHeadlineText:
                        text: "Opional add in Garlic powder"
                    MDListItemSupportingText:
                        text: "and dired basil"
                    MDListItemTrailingCheckbox:
                MDListItem:
                    MDListItemHeadlineText:
                        text: "Add olive oil and warm water"
                    MDListItemSupportingText:
                        text: "and use a wooden spoon to stir well"
                    MDListItemTrailingCheckbox:
                MDListItem:
                    MDListItemHeadlineText:
                        text: "Add another 1 cup of flour"
                    MDListItemSupportingText:
                        text: "stirring until the dough is forming"
                    MDListItemSupportingText:
                        text: "a cohesive elastic ball"
                    MDListItemTrailingCheckbox:
                MDListItem:
                    MDListItemHeadlineText:
                        text: "Add flour as needed"
                    MDListItemSupportingText:
                        text: "dough should be slightly sticky"
                    MDListItemSupportingText:
                        text: "but still manageable with hands"
                    MDListItemTrailingCheckbox:
                MDListItem:
                    MDListItemHeadlineText:
                        text: "Drizzle a serperate large bowl"
                    MDListItemSupportingText:
                        text: "generously with olive oil and use"
                    MDListItemSupportingText:
                        text: "a pastry brush to brush up sides"
                    MDListItemTrailingCheckbox:
                MDListItem:
                    MDListItemHeadlineText:
                        text: "Lightly dust your hands with flour"
                    MDListItemSupportingText:
                        text: "and formyour pizza dough into a"
                    MDListItemSupportingText:
                        text: "ball and transfer to olive oil bowl"
                    MDListItemTrailingCheckbox:
                MDListItem:
                    MDListItemHeadlineText:
                        text: "Roll the pizza dough along the"
                    MDListItemSupportingText:
                        text: "inside of bowl until coated with"
                    MDListItemSupportingText:
                        text: "olive oil"
                    MDListItemTrailingCheckbox:
                MDListItem:
                    MDListItemHeadlineText:
                        text: "Cover with plastic wrap and"
                    MDListItemSupportingText:
                        text: "place in warm place and let"
                    MDListItemSupportingText:
                        text: "rise for 30 minutes or double size"
                    MDListItemTrailingCheckbox:
                MDListItem:
                    MDListItemHeadlineText:
                        text: "Once dough has risen use your"
                    MDListItemSupportingText:
                        text: "hands to gently deflate it"
                    MDListItemTrailingCheckbox:
                MDListItem:
                    MDListItemHeadlineText:
                        text: "Place in a lightly floured bowl"
                    MDListItemSupportingText:
                        text: "wrap and place in fidge for later use"
                    MDListItemTrailingCheckbox:

