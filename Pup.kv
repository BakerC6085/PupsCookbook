<Search>
    MDFloatLayout:
        size_hint: .9, .09
        pos_hint: {"center_x": .5, "center_y": .93}
        radius: [10]
        TextInput:
            id: search_text
            hint_text: "Recipe search..."
            pos_hint: {"center_x": .5, "center_y": .5}
            height: self.minimum_height
            multiline: False
            padding: 15
        MDIconButton:
            icon: "magnify"
            pos_hint: {"center_x": .9, "center_y": .5}
            on_release:
                app.search(search_text.text)

<SearchResults>
    name: "search_results"
    MDIconButton:
        icon: "arrow-left"
        pos_hint: {"center_y": .95}
        on_release:
            screen_manager.current = "Search"
    MDLabel:
        id: search_label
        text: ""
        pos_hint: {"center_y": .95}
        halign: "center"
        font_size: "20sp"

<About>
    MDBoxLayout:
        orientation: "vertical"
        Image:
            source: "images/About.jpg"
            size: "125dp", "250dp"
            halign: "center"
        ScrollView:
            do_scroll_x: False
            MDGridLayout:
                cols: 1
                col_force_default: False
                spacing: dp(6)
                adaptive_height: True
                padding: dp(6)
                MDListItem:
                    MDListItemHeadlineText:
                        text: "In the memory of Zac and Con"
                    MDListItemSupportingText:
                        text: "Rest in Peace"
                MDListItem:
                    MDListItemHeadlineText:
                        text: "We will always remember"
                    MDListItemSupportingText:
                        text: "April 5th, 2024"
                MDListItem:
                    MDListItemHeadlineText:
                        text: "If you ever need help or support,"
                    MDListItemSupportingText:
                        text: "please reach out"
                MDListItem:
                    MDListItemHeadlineText:
                        text: "Suicide hotline:"
                    MDListItemSupportingText:
                        text: "call or text 988"
                MDListItem:
                    MDListItemHeadlineText:
                        text: "Domestic Violence Hotline:"
                    MDListItemSupportingText:
                        text: "Call 800-799-7233"
                MDListItem:
                    MDListItemHeadlineText:
                        text: "Human Trafficking Hotline"
                    MDListItemSupportingText:
                        text: "Call 888-373-7888"
                
                
<Login>
    MDLabel:
        text: "Welcome"
        halign: "center"
        theme_text_color: "Custom"
        text_color: "blue"
        font_style: "Display"
        role: "large"
        pos_hint: {"center_x": .5, "center_y": .8}

    MDTextField:
        id: email
        mode: "outlined"
        size_hint_x: None
        width: "240dp"
        pos_hint: {"center_x": .5, "center_y": .65}

        MDTextFieldLeadingIcon:
            icon: "account"

        MDTextFieldHintText:
            text: "email"

        MDTextFieldTrailingIcon:
            icon: "information"

    MDTextField:
        id: password
        mode: "outlined"
        size_hint_x: None
        width: "240dp"
        pos_hint: {"center_x": .5, "center_y": .5}

        MDTextFieldLeadingIcon:
            icon: "eye"

        MDTextFieldHintText:
            text: "Password"

        MDTextFieldTrailingIcon:
            icon: "information"

    MDButton:
        style: "filled"
        pos_hint: {"center_x": .30, "center_y": .35}

        MDButtonText:
            text: "Register"

    MDButton:
        style: "filled"
        pos_hint: {"center_x": .70, "center_y": .35}

        MDButtonText:
            text: "Login"

