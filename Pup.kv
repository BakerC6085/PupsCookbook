<Biscuits>
    MDBoxLayout:
        orientation: "vertical"
        ScrollView:
            do_scroll_x: False
            MDGridLayout:
                cols: 1
                col_force_default: False
                spacing: dp(6)
                adaptive_height: True
                padding: dp(6)
                MDListItem:
                    MDListItemHeadlineText:
                        text: "Biscuits"
                        font_style: "Display"
                        role: "medium"
                MDListItem:
                    MDListItemHeadlineText:
                        text: "Ingredients"
                        font_style: "Headline"
                        role: "medium"
                MDListItem:
                    MDListItemHeadlineText:
                        text: "2 Cups all purpose flour"
                    MDListItemTrailingCheckbox:
                MDListItem:
                    MDListItemHeadlineText:
                        text: "1 tbsp Baking Powder"
                    MDListItemTrailingCheckbox:
                MDListItem:
                    MDListItemHeadlineText:
                        text: "1 tbsp granulated Sugar"
                    MDListItemTrailingCheckbox:
                MDListItem:
                    MDListItemHeadlineText:
                        text: "1 tsp salt"
                    MDListItemTrailingCheckbox:
                MDListItem:
                    MDListItemHeadlineText:
                        text: "6 tbsp Unsalted Butter"
                    MDListItemSupportingText:
                        text: "very cold"
                    MDListItemTrailingCheckbox:
                MDListItem:
                    MDListItemHeadlineText:
                        text: "3/4 cups Whole Milk"
                    MDListItemSupportingText:
                        text: "butermilk or 2% sub"
                    MDListItemTrailingCheckbox:
                MDListItem:
                    MDListItemHeadlineText:
                        text: "Directions"
                        font_style: "Headline"
                        role: "medium"
                MDListItem:
                    MDListItemHeadlineText:
                        text: "For best results chill butter"
                    MDListItemSupportingText:
                        text: "in freezer for 10 to 20 minutes"
                    MDListItemTrailingCheckbox:
                MDListItem:
                    MDListItemHeadlineText:
                        text: "Preheat oven to 425 degrees F"
                    MDListItemSupportingText:
                        text: "line baking sheet with nonstick"
                    MDListItemTertiaryText:
                        text: "parchment paper"
                    MDListItemTrailingCheckbox:
                MDListItem:
                    MDListItemHeadlineText:
                        text: "Combine flour baking powder"
                    MDListItemSupportingText:
                        text: "sugar and salt in a large bowl"
                    MDListItemTertiaryText:
                        text: "and mix"
                    MDListItemTrailingCheckbox:
                MDListItem:
                    MDListItemHeadlineText:
                        text: "Grate butter into Flour mixture"
                    MDListItemSupportingText:
                        text: "using grater or food processor"
                    MDListItemTertiaryText:
                        text: "and mix"
                    MDListItemTrailingCheckbox:
                MDListItem:
                    MDListItemHeadlineText:
                        text: "Combine until mixture looks"
                    MDListItemSupportingText:
                        text: "like crumbs"
                    MDListItemTrailingCheckbox:
                MDListItem:
                    MDListItemHeadlineText:
                        text: "And milk, use spatula to stir"
                    MDListItemSupportingText:
                        text: "until combined"
                    MDListItemTertiaryText:
                        text: "Do not overwork dough"
                    MDListItemTrailingCheckbox:
                MDListItem:
                    MDListItemHeadlineText:
                        text: "Transfer dough to well floured"
                    MDListItemSupportingText:
                        text: "surface and use hands to gently"
                    MDListItemTertiaryText:
                        text: "work dough together"
                    MDListItemTrailingCheckbox:
                MDListItem:
                    MDListItemHeadlineText:
                        text: "Fold dough in half over itself"
                    MDListItemSupportingText:
                        text: "gently flatten layers together"
                    MDListItemTertiaryText:
                        text: "Rotate dough and repeat 5 times"
                    MDListItemTrailingCheckbox:
                MDListItem:
                    MDListItemHeadlineText:
                        text: "Use Hands to flatten dough"
                    MDListItemSupportingText:
                        text: "To 1 inch thick, dust 2 3/4"
                    MDListItemTertiaryText:
                        text: "inch biscuit cutter with flour"
                    MDListItemTrailingCheckbox:
                MDListItem:
                    MDListItemHeadlineText:
                        text: "Making close cuts, press biscuit"
                    MDListItemSupportingText:
                        text: "cutter into dough, remove biscuit"
                    MDListItemSupportingText:
                        text: "and place on prepared baking sheet"
                    MDListItemTrailingCheckbox:
                MDListItem:
                    MDListItemHeadlineText:
                        text: "Repeat until you have gotten"
                    MDListItemSupportingText:
                        text: "as many biscuits as possible"
                    MDListItemSupportingText:
                        text: "places 1/2 inch apart on baking sheet"
                    MDListItemTrailingCheckbox:
                MDListItem:
                    MDListItemHeadlineText:
                        text: "Gently rework dough"
                    MDListItemSupportingText:
                        text: "to get another one or two biscuits"
                    MDListItemTrailingCheckbox:
                MDListItem:
                    MDListItemHeadlineText:
                        text: "Bake on 425 degrees F"
                    MDListItemSupportingText:
                        text: "for 12 minutes or until tops"
                    MDListItemSupportingText:
                        text: "are lightly golden brown"
                    MDListItemTrailingCheckbox:
                MDListItem:
                    MDListItemHeadlineText:
                        text: "If desired, brush with"
                    MDListItemSupportingText:
                        text: "melted salted butter immediately"
                    MDListItemSupportingText:
                        text: "after removing from oven"
                    MDListItemTrailingCheckbox:


