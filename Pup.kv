                MDListItem:
                    MDListItemHeadlineText:
                        text: "Pizza Sauce"
                    MDListItemSupportingText:
                        text: "warm, see pizza sauce recipe"
                    MDListItemTrailingCheckbox:
                MDListItem:
                    MDListItemHeadlineText:
                        text: "Directions"
                        font_style: "Headline"
                        role: "medium"
                MDListItem:
                    MDListItemHeadlineText:
                        text: "Place butter parmesan cheese"
                    MDListItemSupportingText:
                        text: "garlic powder and Italian seasoning"
                    MDListItemSupportingText:
                        text: "in a bowl and mix together until smooth"
                    MDListItemTrailingCheckbox:
                MDListItem:
                    MDListItemHeadlineText:
                        text: "Spread the butter mixture on"
                    MDListItemSupportingText:
                        text: "one side of the bread slices"
                    MDListItemSupportingText:
                        text: "Place butte side up on paper towel"
                    MDListItemTrailingCheckbox:
                MDListItem:
                    MDListItemHeadlineText:
                        text: "Heat skillet on medium-low heat"
                    MDListItemHeadlineText:
                        text: "place bread butter side down"
                    MDListItemTrailingCheckbox:
                MDListItem:
                    MDListItemHeadlineText:
                        text: "place a slice of mozzarella"
                    MDListItemSupportingText:
                        text: "on bread, add 6 pepperoni slices"
                    MDListItemSupportingText:
                        text: "repeat once"
                    MDListItemTrailingCheckbox:
                MDListItem:
                    MDListItemHeadlineText:
                        text: "Add one more slice of Mozzarella"
                    MDListItemSupportingText:
                        text: "Place on top bread butter side up"
                    MDListItemTrailingCheckbox:
                MDListItem:
                    MDListItemHeadlineText:
                        text: "place lid on skillet, melt cheese"
                    MDListItemSupportingText:
                        text: "cook 3 to 5 minutes till golden brown"
                    MDListItemSupportingText:
                        text: "flip, relplace lid"
                    MDListItemTrailingCheckbox:
                MDListItem:
                    MDListItemHeadlineText:
                        text: "cook until second side is"
                    MDListItemSupportingText:
                        text: "golden brown"
                    MDListItemTrailingCheckbox:
                MDListItem:
                    MDListItemHeadlineText:
                        text: "Serve with pizza sauce"
                    MDListItemTrailingCheckbox:

