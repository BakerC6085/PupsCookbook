                    MDListItemSupportingText:
                        text: "please see Pepperoni recipe"
                    MDListItemTrailingCheckbox:
                MDListItem:
                    MDListItemHeadlineText:
                        text: "6oz Mozzarella Cheese"
                    MDListItemSupportingText:
                        text: "low moisture, cut into 12 pieces"
                    MDListItemTrailingCheckbox:
                MDListItem:
                    MDListItemHeadlineText:
                        text: "4 tbsp Unsalted Butter"
                    MDListItemSupportingText:
                        text: "melted"
                    MDListItemTrailingCheckbox:
                MDListItem:
                    MDListItemHeadlineText:
                        text: "1/4 tsp Garlic Powder"
                    MDListItemTrailingCheckbox:
                MDListItem:
                    MDListItemHeadlineText:
                        text: "1/2 Kosher Salt"
                    MDListItemTrailingCheckbox:
                MDListItem:
                    MDListItemHeadlineText:
                        text: "1/4 cup Parmesan Cheese"
                    MDListItemSupportingText:
                        text: "grated"
                    MDListItemSupportingText:
                        text: "devided"
                    MDListItemTrailingCheckbox:
                MDListItem:
                    MDListItemHeadlineText:
                        text: "1 1/2 tsp Italian seasoning"
                    MDListItemSupportingText:
                        text: "divided"
                    MDListItemTrailingCheckbox:
                MDListItem:
                    MDListItemHeadlineText:
                        text: "Instructions"
                        font_style: "Headline"
                        role: "medium"
                MDListItem:
                    MDListItemHeadlineText:
                        text: "Arrange rack into middle of oven"
                    MDListItemSupportingText:
                        text: "preheat 375 degrees F"
                    MDListItemSupportingText:
                        text: "grease 12 cup muffin tin"
                    MDListItemTrailingCheckbox:
                MDListItem:
                    MDListItemHeadlineText:
                        text: "Divide pizza dough into 12"
                    MDListItemSupportingText:
                        text: "portions, roll each piece into a ball"
                    MDListItemSupportingText:
                        text: "flatten each to even 3 to 4 inch round"
                    MDListItemTrailingCheckbox:
                MDListItem:
                    MDListItemHeadlineText:
                        text: "place 1/2 tsp of marinara sauce"
                    MDListItemSupportingText:
                        text: "into center of each dough"
                    MDListItemTrailingCheckbox:
                MDListItem:
                    MDListItemHeadlineText:
                        text: "Top with a piece of"
                    MDListItemSupportingText:
                        text: "mozzarella and 2 slices of"
                    MDListItemSupportingText:
                        text: "pepperoni"
                    MDListItemTrailingCheckbox:
                MDListItem:
                    MDListItemHeadlineText:
                        text: "Fold edges of the dough"
                    MDListItemSupportingText:
                        text: "up and around the filling and pinch"
                    MDListItemSupportingText:
                        text: "together forming a ball"
                    MDListItemTrailingCheckbox:
                MDListItem:
                    MDListItemHeadlineText:
                        text: "Stir butter garlic and salt"
                    MDListItemSupportingText:
                        text: "together in a small bowl"
                    MDListItemTrailingCheckbox:
                MDListItem:
                    MDListItemHeadlineText:
                        text: "Dip the pizza balls"
                    MDListItemSupportingText:
                        text: "one at a time, turning to coat"
                    MDListItemSupportingText:
                        text: "Place seam down in muffin tin"
                    MDListItemTrailingCheckbox:
                MDListItem:
