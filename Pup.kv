                    MDListItemSupportingText:
                        text: "fat included, cubed"
                    MDListItemTrailingCheckbox:
                MDListItem:
                    MDListItemHeadlineText:
                        text: "3 pounds Lean Beef Chuck"
                    MDListItemSupportingText:
                        text: "round or shank, cubed"
                    MDListItemTrailingCheckbox:
                MDListItem:
                    MDListItemHeadlineText:
                        text: "5 tbsp Salt"
                    MDListItemTrailingCheckbox:
                MDListItem:
                    MDListItemHeadlineText:
                        text: "1 tbsp Sugar"
                    MDListItemTrailingCheckbox:
                MDListItem:
                    MDListItemHeadlineText:
                        text: "2 tbsp Cayenne Pepper"
                    MDListItemTrailingCheckbox:
                MDListItem:
                    MDListItemHeadlineText:
                        text: "3 tbsp Sweet Paprika"
                    MDListItemTrailingCheckbox:
                MDListItem:
                    MDListItemHeadlineText:
                        text: "1 tbsp Anise Seed"
                    MDListItemSupportingText:
                        text: "crushed"
                    MDListItemTrailingCheckbox:
                MDListItem:
                    MDListItemHeadlineText:
                        text: "1 tsp Minced Garlic"
                    MDListItemSupportingText:
                        text: "very fine"
                    MDListItemTrailingCheckbox:
                MDListItem:
                    MDListItemHeadlineText:
                        text: "1 cup Dry Red Wine"
                    MDListItemTrailingCheckbox:
                MDListItem:
                    MDListItemHeadlineText:
                        text: "1/2 tsp Ascorbix Acid"
                    MDListItemTrailingCheckbox:
                MDListItem:
                    MDListItemHeadlineText:
                        text: "1 tsp of Saltpepper"
                    MDListItemTrailingCheckbox:
                MDListItem:
                    MDListItemHeadlineText:
                        text: "4 feet sauage casing"
                    MDListItemTrailingCheckbox:
                MDListItem:
                    MDListItemHeadlineText:
                        text: "1 tbsp White Vinegar"
                    MDListItemSupportingText:
                        text: "more or less"
                    MDListItemTrailingCheckbox:
                MDListItem:
                    MDListItemHeadlineText:
                        text: "Directions"
                        font_style: "Headline"
                        role: "medium"
                MDListItem:
                    MDListItemHeadlineText:
                        text: "Separtely grind pork and beef"
                    MDListItemSupportingText:
                        text: "through the coarse disk"
                    MDListItemTrailingCheckbox:
                MDListItem:
                    MDListItemHeadlineText:
                        text: "Mix meat together with salt sugar"
                    MDListItemSupportingText:
                        text: "cayenne paprika anise seed garlic"
                    MDListItemSupportingText:
                        text: "red wine ascorbic acid saltpepper"
                    MDListItemTrailingCheckbox:
                MDListItem:
                    MDListItemHeadlineText:
                        text: "Spread mixture on large pan"
                    MDListItemSupportingText:
                        text: "cover loosely with waxed paper"
                    MDListItemSupportingText:
                        text: "cure in refigerator for 24 hours"
                    MDListItemTrailingCheckbox:
                MDListItem:
                    MDListItemHeadlineText:
                        text: "Prepare the casing"
                    MDListItemSupportingText:
                        text: "rinse casing under cool running water"
                    MDListItemTrailingCheckbox:
                MDListItem:
                    MDListItemHeadlineText:
                        text: "Place casing in a bowl of"
                    MDListItemSupportingText:
                        text: "cool water and let soak for 30 minutes"
                    MDListItemTrailingCheckbox:
                MDListItem:
                    MDListItemHeadlineText:
                        text: "rinse casing under cool water"
                    MDListItemSupportingText:
                        text: "slip 1 end over faucet adn turn water"
                    MDListItemSupportingText:
                        text: "on slowly then more focefully"
                    MDListItemTrailingCheckbox:
                MDListItem:
                    MDListItemHeadlineText:
                        text: "Pinpoint if the casing has any breaks"
                    MDListItemHeadlineText:
                        text: "if so cut out that portion"
                    MDListItemTrailingCheckbox:
                MDListItem:
                    MDListItemHeadlineText:
                        text: "Place casing in bowl of water"
                    MDListItemSupportingText: 
                        text: "add white vinegar"
                    MDListItemSupportingText:
                        text: "1 tbsp per cup of water"
                    MDListItemTrailingCheckbox:
                MDListItem:
                    MDListItemHeadlineText:
                        text: "Rinse casing"
                    MDListItemTrailingCheckbox:
                MDListItem:
                    MDListItemHeadlineText:
                        text: "Stuff sausage into casing"
                    MDListItemSupportingText:
                        text: "twist off into 10 inch links"
                    MDListItemTrailingCheckbox:
                MDListItem:
                    MDListItemHeadlineText:
                        text: "Using cotton twine"
                    MDListItemSupportingText:
                        text: "tie two links together"
                    MDListItemTrailingCheckbox:
                MDListItem:
                    MDListItemHeadlineText:
                        text: "Hang to dry in a space"
                    MDListItemSupportingText:
                        text: "that is 50 to 55 degrees F"
                    MDListItemSupportingText:
                        text: "with 75 to 80 percent humidity"
                    MDListItemTrailingCheckbox:
                MDListItem:
                    MDListItemHeadlineText: 
                        text: "Leave for six to eight weeks"
                    MDListItemTrailingCheckbox:


