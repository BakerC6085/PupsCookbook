<CreamsicleCookies>
    MDBoxLayout:
        orientation: "vertical"
        ScrollView:
            do_scroll_x: False
            MDGridLayout:
                cols: 1
                col_force_default: False
                spacing: dp(6)
                adaptive_height: True
                padding: dp(6)
                MDListItem:
                    MDListItemHeadlineText:
                        text: "Creamsicle"
                        font_style: "Display"
                        role: "medium"
                MDListItem:
                    MDListItemHeadlineText:
                        text: "Cookies"
                        font_style: "Display"
                        role: "medium"
                MDListItem:
                    MDListItemHeadlineText:
                        text: "Ingredients"
                        font_style: "Headline"
                        role: "medium"
                MDListItem:
                    MDListItemHeadlineText:
                        text: "15.25 oz White Cake Mix"
                    MDListItemSupportingText:
                        text: "see White Cake Mix recipe"
                    MDListItemTrailingCheckbox:
                MDListItem:
                    MDListItemHeadlineText:
                        text: "3 oz orange jello"
                    MDListItemTrailingCheckbox:
                MDListItem:
                    MDListItemHeadlineText:
                        text: "2 tbsp Orange Zest"
                    MDListItemTrailingCheckbox:
                MDListItem:
                    MDListItemHeadlineText:
                        text: "8 tbsp Salted Butter"
                    MDListItemSupportingText:
                        text: "softened"
                    MDListItemTrailingCheckbox:
                MDListItem:
                    MDListItemHeadlineText:
                        text: "2 large Eggs"
                    MDListItemSupportingText:
                        text: "room temp"
                    MDListItemTrailingCheckbox:
                MDListItem:
                    MDListItemHeadlineText:
                        text: "11 oz White Chocolate chips"
                    MDListItemTrailingCheckbox:
                MDListItem:
                    MDListItemHeadlineText:
                        text: "Directions"
                        font_style: "Headline"
                        role: "medium"
                MDListItem:
                    MDListItemHeadlineText:
                        text: "Preheat oven to 350 degrees F"
                    MDListItemSupportingText:
                        text: "line two baing sheets with"
                    MDListItemSupportingText:
                        text: "parchment paper"
                    MDListItemTrailingCheckbox:
                MDListItem:
                    MDListItemHeadlineText:
                        text: "Mix cake mix orange jello and"
                    MDListItemSupportingText:
                        text: "orange zest until fully combined"
                    MDListItemTrailingCheckbox:
                MDListItem:
                    MDListItemHeadlineText:
                        text: "Add butter and eggs to the bowl"
                    MDListItemSupportingText:
                        text: "Use hand mixer or stand mixer"
                    MDListItemSupportingText:
                        text: "to beat together about 2 minutes"
                    MDListItemTrailingCheckbox:
                MDListItem:
                    MDListItemHeadlineText:
                        text: "Once combined add white"
                    MDListItemSupportingText:
                        text: "chocolate chips and fold into"
                    MDListItemSupportingText:
                        text: "the dough not with mixer"
                    MDListItemTrailingCheckbox:
                MDListItem:
                    MDListItemHeadlineText:
                        text: "Use a 1 1/2 tbsp cookie scoop"
                    MDListItemSupportingText:
                        text: "to scoop out dough, roll into ball"
                    MDListItemTrailingCheckbox:
                MDListItem:
                    MDListItemHeadlineText:
                        text: "Place balls 2 inches apart"
                    MDListItemSupportingText:
                        text: "Bake for 10 to 13 Minutes"
                    MDListItemTrailingCheckbox:
                MDListItem:
                    MDListItemHeadlineText:
                        text: "Once edges are set remove"
                    MDListItemSupportingText:
                        text: "from oven and let cool for 10 minutes"
                    MDListItemTrailingCheckbox:

