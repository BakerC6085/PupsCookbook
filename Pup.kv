<LoafBread>
    MDBoxLayout:
        orientation: "vertical"
        ScrollView:
            do_scroll_x: False
            MDGridLayout:
                cols: 1
                col_force_default: False
                spacing: dp(6)
                adaptive_height: True
                padding: dp(6)
                MDListItem:
                    MDListItemHeadlineText:
                        text: "Loaf Bread"
                        font_style: "Display"
                        role: "medium"
                MDListItem:
                    MDListItemHeadlineText:
                        text: "Ingredients"
                        font_style: "Headline"
                        role: "medium"
                MDListItem:
                    MDListItemHeadlineText:
                        text: "2 cups warm water"
                    MDListItemSupportingText:
                        text: "105 to 115 degrees F"
                    MDListItemTrailingCheckbox:
                MDListItem:
                    MDListItemHeadlineText:
                        text: "1 tbsp active dry yeast"
                    MDListItemTrailingCheckbox:
                MDListItem:
                    MDListItemHeadlineText:
                        text: "1/4 cup Honey or Sugar"
                    MDListItemTrailingCheckbox:
                MDListItem:
                    MDListItemHeadlineText:
                        text: "2 tsp Salt"
                    MDListItemTrailingCheckbox:
                MDListItem:
                    MDListItemHeadlineText:
                        text: "2 tbsp Oil"
                    MDListItemSupportingText:
                        text: "canola or vegetable"
                    MDListItemTrailingCheckbox:
                MDListItem:
                    MDListItemHeadlineText:
                        text: "4 - 5 1/2 cups flour"
                    MDListItemSupportingText:
                        text: "all purpose or bread"
                    MDListItemTrailingCheckbox:
                MDListItem:
                    MDListItemHeadlineText:
                        text: "Directions"
                        font_style: "Headline"
                        role: "medium"
                MDListItem:
                    MDListItemHeadlineText:
                        text: "Prepare the dough"
                    MDListItemSupportingText:
                        text: "in large bowl mix yeast water and"
                    MDListItemSupportingText:
                        text: "a pinch of sugar or honey"
                    MDListItemTrailingCheckbox:
                MDListItem:
                    MDListItemHeadlineText:
                        text: "Allow to rest for 5 to 10 minutes"
                    MDListItemSupportingText:
                        text: "until foaming and bubbly"
                    MDListItemTrailingCheckbox:
                MDListItem:
                    MDListItemHeadlineText:
                        text: "Add remaining Sugar or Honey"
                    MDListItemSupportingText:
                        text: "salt oil and 3 cups of flour"
                    MDListItemSupportingText:
                        text: "Mix to combine"
                    MDListItemTrailingCheckbox:
                MDListItem:
                    MDListItemHeadlineText:
                        text: "Add another cup of flour"
                    MDListItemSupportingText:
                        text: "mix to combine"
                    MDListItemTrailingCheckbox:
                MDListItem:
                    MDListItemHeadlineText:
                        text: "With mixer running add"
                    MDListItemSupportingText:
                        text: "1/2 cup flour to mixure"
                    MDListItemSupportingText:
                        text: "Repeat till dough pulls from sides"
                    MDListItemTrailingCheckbox:
                MDListItem:
                    MDListItemHeadlineText:
                        text: "Check that dough is smooth"
                    MDListItemSupportingText:
                        text: "and elastic slightly sticky to a clean"
                    MDListItemSupportingText:
                        text: "finger, if not add more flour"
                    MDListItemTrailingCheckbox:
                MDListItem:
                    MDListItemHeadlineText:
                        text: "Knead the Dough"
                    MDListItemSupportingText:
                        text: "Mix dough for 4 to 5 minutes on medium"
                    MDListItemTrailingCheckbox:
                MDListItem:
                    MDListItemHeadlineText:
                        text: "First Rise"
                    MDListItemSupportingText:
                        text: "grease large bowl with oil"
                    MDListItemSupportingText:
                        text: "place dough inside turning to coat"
                    MDListItemTrailingCheckbox:
                MDListItem:
                    MDListItemHeadlineText:
                        text: "Cover with dish towel"
                    MDListItemSupportingText:
                        text: "or plastic wrap and allow to rise"
                    MDListItemSupportingText:
                        text: "in a warm place until double in size"
                    MDListItemTrailingCheckbox:
                MDListItem:
                    MDListItemHeadlineText:
                        text: "Spray 2 9x5 bread pans"
                    MDListItemTrailingCheckbox:
                MDListItem:
                    MDListItemHeadlineText:
                        text: "Punch dough down"
                    MDListItemSupportingText:
                        text: "remove the air bubbles"
                    MDListItemTrailingCheckbox:
                MDListItem:
                    MDListItemHeadlineText:
                        text: "Divde into two equal portions"
                    MDListItemSupportingText:
                        text: "shape each ball into long logs" 
                    MDListItemSupportingText:
                        text: "place into greased loaf pans"
                    MDListItemTrailingCheckbox:
                MDListItem:
                    MDListItemHeadlineText:
                        text: "Second Rise"
                    MDListItemSupportingText:
                        text: "spray plastic wrap with cooking spray"
                    MDListItemSupportingText:
                        text: "cover pans"
                    MDListItemTrailingCheckbox:
                MDListItem:
                    MDListItemHeadlineText:
                        text: "Allow to rise for 45 minutes"
                    MDListItemSupportingText:
                        text: "to 1 hour or until 1 inch above"
                    MDListItemSupportingText:
                        text: "the loaf pans. Gently remove cover"
                    MDListItemTrailingCheckbox:
                MDListItem:
                    MDListItemHeadlineText:
                        text: "Bake"
                    MDListItemSupportingText:
                        text: "preheat oven to 350 F"
                    MDListItemTrailingCheckbox:
                MDListItem:
                    MDListItemHeadlineText:
                        text: "Bake for 30 to 33 minutes"
                    MDListItemSupportingText:
                        text: "or until golden brown"
                    MDListItemTrailingCheckbox:
                MDListItem:
                    MDListItemHeadlineText:
                        text: "Give the top of the loaf"
                    MDListItemSupportingText:
                        text: "a gentle tap, it should sound"
                    MDListItemSupportingText:
                        text: "hollow"
                    MDListItemTrailingCheckbox:
                MDListItem:
                    MDListItemHeadlineText:
                        text: "Invert loafs on cooling rack"
                    MDListItemSupportingText:
                        text: "brush tops with butter"
                    MDListItemSupportingText:
                        text: "allow to cool for 10 minutes"
                    MDListItemTrailingCheckbox:
                MDListItem:
                    MDListItemHeadlineText:
                        text: "Slice the loafs"
                    MDListItemSupportingText:
                        text: "store in airtight container"
                    MDListItemSupportingText:
                        text: "good for 2-3 days room temp"
                    MDListItemTrailingCheckbox:

