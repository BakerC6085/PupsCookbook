<BaseMDNavigationItem>

    MDNavigationItemIcon:
        icon: root.icon

    MDNavigationItemLabel:
        text: root.text

<Home>
    MDBoxLayout:
        orientation: "vertical"
        Image:
            source: "images/HumanPup.png"
        ScrollView:
            do_scroll_x: False
            MDGridLayout:
                cols: 1
                col_force_default: False
                spacing: dp(6)
                adaptive_height: True
                padding: dp(6)
                MDListItem:
                    MDListItemHeadlineText:
                        text: "The Pup's Cookbook"
                        font_style: "Headline"
                        role: "medium"
                        halign: "center"
                    MDListItemSupportingText:
                        text: "Satisfy all your wants!"
                        font_style: "Headline"
                        role: "small"
                        halign: "center"

